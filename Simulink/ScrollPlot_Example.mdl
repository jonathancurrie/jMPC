      Position		      [315, 27, 365, 103]
      FunctionName	      "sfun_scroll"
      Parameters	      "Ts,Frame_Size,Update_Delay,y_lim,setp"
      MaskType		      "Scrolling Plot"
      MaskDescription	      "Create a scrolling, autoscaling display for use in Simulink\n\nThe display is a discrete "
      "component, and requires a sampling period. Adjust this period for the desired resolution.\n\nFrame size refers t"
      "o the number of samples to be displayed on the screen before it begins to scroll.\n\nUpdate delay allows a rough"
      " delay of the simulation execution period, allowing the user to slow down the display."
      MaskHelp		      "Note this block accepts a maximum of one input per port, if you connect a multiple dimsension s"
      "ignal results will be unpredictable."
      MaskPromptString	      "Sampling Time (Seconds)|Frame Size (Samples)|Update Delay (Seconds)|Initial Y axis Limit"
      "s [ymin ymax]|Enable Set Point Input"
      MaskStyleString	      "edit,edit,edit,edit,checkbox"
      MaskTunableValueString  "on,on,on,on,on"
      MaskCallbackString      "||||"
      MaskEnableString	      "on,on,on,on,on"
      MaskVisibilityString    "on,on,on,on,on"
      MaskToolTipString	      "on,on,on,on,on"
      MaskVarAliasString      ",,,,"
      MaskVariables	      "Ts=@1;Frame_Size=@2;Update_Delay=@3;y_lim=@4;setp=@5;"
      MaskInitialization      "if(~Ts)\n    disp('Warning: Sampling Time (Ts) Not Specifed or is Zero');\n    Ts = 0.1"
      ";\nend\n\nif(~Frame_Size)\n    disp('Warning: Frame Size Not Specifed or is Zero');\n    Frame_Size = 100;\nend\n"
      "\nif(Update_Delay < 0)\n    Update_Delay = 0;\nelse if(Update_Delay > 10)\n        disp('Warning: Update Delay S"
      "pecified as > 10'); \n        Update_Delay = 10;\n    end\nend\n\nif(y_lim(1) > y_lim(2))\n    disp('Warning: Li"
      "mit 1 is greater than Limit 2');\n    y_lim(1) = -y_lim(1);\nend\nif(y_lim(1) == y_lim(2))\n    disp('Warning: L"
      "imits are equal');\n    if(~y_lim(1))\n        y_lim(2) = 1;\n    else\n        y_lim(2) = y_lim(2)+abs(y_lim(2)"
      ");\n    end\nend"
      MaskDisplay	      "image(imread('Scrollpic.jpg'))"
      MaskIconFrame	      off
      MaskIconOpaque	      on
      MaskIconRotate	      "none"
      MaskPortRotate	      "default"
      MaskIconUnits	      "autoscale"
      MaskValueString	      "0.1|200|0.005|[-1 1]|on"
      MaskTabNameString	      ",,,,"
    }
